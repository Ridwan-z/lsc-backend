{
    "version": 2,
    "framework": null,
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.7.1",
            "maxDuration": 10
        }
    },
    "routes": [
        {
            "src": "/(css|js|images|favicon.ico)/(.*)",
            "dest": "/public/$1/$2"
        },
        {
            "src": "/api/(.*)",
            "dest": "/api/index.php"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "buildCommand": "composer install --no-dev --optimize-autoloader",
    "installCommand": "echo 'Using buildCommand instead'",
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "LOG_CHANNEL": "stderr",
        "SESSION_DRIVER": "array",
        "CACHE_STORE": "array"
    }
}
