{
    "version": 2,
    "builds": [{ "src": "api/index.php", "use": "vercel-php@0.7.1" }],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/index.php"
        },
        {
            "src": "/api",
            "dest": "/api/index.php"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ]
}
